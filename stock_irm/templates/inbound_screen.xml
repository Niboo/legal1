            <!-- jQuery stuff -->
            <script type="text/javascript" src="/web/static/lib/jquery/jquery.js"></script>
            <!--<script type="text/javascript" src="/web/static/lib/jquery.blockUI/jquery.blockUI.js"></script>-->
            <!--<script type="text/javascript" src="/web/static/lib/jquery.hotkeys/jquery.hotkeys.js"></script>-->
            <!--<script type="text/javascript" src="/web/static/lib/jquery.placeholder/jquery.placeholder.js"></script>-->
            <!--<script type="text/javascript" src="/web/static/lib/jquery.timeago/jquery.timeago.js"></script>-->
            <!--<script type="text/javascript" src="/web/static/lib/jquery.form/jquery.form.js"></script>-->

            <!--<script type="text/javascript" src="/web/static/lib/jquery.ba-bbq/jquery.ba-bbq.js"></script>-->

            <script type="text/javascript" src="/web/static/lib/qweb/qweb2.js"></script>
            <script type="text/javascript" src="/web/static/src/js/openerpframework.js"></script>
            <!--<script type="text/javascript" src="/web/static/src/js/tour.js"></script>-->

            <!--BOOTSTRAP-->
            <link id="bootstrap_css" rel='stylesheet' href='/web/static/lib/bootstrap/css/bootstrap.css' />
            <script type="text/javascript" src="/web/static/lib/bootstrap/js/bootstrap.js"></script>

            <link rel="stylesheet" href="/web/static/lib/fontawesome/css/font-awesome.css"/>

            <!--STOCK IRM-->
            <link href="/stock_irm/static/src/css/template.css" rel="stylesheet"/>
            <script type="text/javascript" src="/stock_irm/static/src/js/stock_irm_widget.js"></script>
            <script type="text/javascript" src="/stock_irm/static/src/js/inbound_supplier_selector.js"></script>
            <script type="text/javascript" src="/stock_irm/static/src/js/inbound_product_picking.js"></script>
            <script type="text/javascript" src="/stock_irm/static/src/js/inbound_product_page.js"></script>
        </template>

        <template id="stock_irm.layout" name="Stock IRM Header">
            <html lang="en">
                <head>
                    <meta charset="UTF-8"/>
                    <title>Inbound</title>
                    <t t-call-assets="stock_irm.assets" t-js="false"/>
                </head>
                <body>
                    <div class="modal fade" id="modalWindow" tabindex="-1" role="dialog">
                        <div class="modal-dialog" role="document">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h4 class="modal-title" />
                                </div>
                                <div class="modal-body" />
                                <div class="modal-footer" />
                            </div>
                        </div>
                    </div>
                    <div>
                        <nav class="navbar navbar-default">
                            <div class="collapse navbar-collapse">
                                <ul class="nav navbar-nav navbar-left">
                                    <li id="back"><a href="#"><span class="glyphicon glyphicon-chevron-left" /></a></li>
                                    <!--<li role="separator" class="divider" />-->
                                </ul>
                                <ul class="nav navbar-nav navbar-center">
                                    <li><a href="#"><span class="glyphicon glyphicon-user"/> <t t-esc="user_name"/></a></li>
                                </ul>
                                <ul class="nav navbar-nav navbar-right">
                                    <li id="search"><a href="#"><span class="glyphicon glyphicon-search" /> Search</a></li>
                                    <li id="confirm" class="navbar-ok"><a href="#"><span class="glyphicon glyphicon-ok" /> Confirm</a></li>
                                </ul>
                            </div>
                        </nav>
                    </div>
                    <div id="content">
                        <t t-raw="0"/>
                    </div>
                    <t t-call-assets="stock_irm.assets" t-css="false"/>
                </body>
            </html>
        </template>

        <template id="stock_irm.inbound_screen" name="Inbound Screen">
            <t t-call="stock_irm.layout">
            </t>
        </template>
    </data>
</openerp>
