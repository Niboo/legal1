<?xml version="1.0"?>
<openerp>
    <data>
        <record id="view_inbound_wave" model="ir.ui.view">
            <field name="name">stock.inbound.wave.form</field>
            <field name="model">stock.inbound.wave</field>
            <field name="arch" type="xml">
                <form string="Stock Inbound Wave">
                    <sheet>
                        <group>
                            <field name="user_id" />
                            <field name="state" />
                        </group>

                        <field name="package_ids"/>

                    </sheet>
                </form>
            </field>
        </record>

        <record id="view_inbound_wave_tree" model="ir.ui.view">
            <field name="name">stock.inbound.wave.tree</field>
            <field name="model">stock.inbound.wave</field>
            <field name="arch" type="xml">
                <tree string="Stock Inbound Wave">
                    <field name="user_id" />
                    <field name="state" />
                    <field name="package_ids" />
                </tree>
            </field>
        </record>

        <record id="inbound_wave_action" model="ir.actions.act_window">
            <field name="name">Inbound Wave</field>
            <field name="res_model">stock.inbound.wave</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
        </record>

        <menuitem action="inbound_wave_action" id="menu_inbound_wave"
            parent="stock.menu_stock_warehouse_mgmt" sequence="100"/>

    </data>
</openerp>
