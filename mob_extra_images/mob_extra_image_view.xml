<?xml version="1.0" encoding="utf-8"?>

<openerp>
	<data>	
        <record id="view_product_image_tree" model="ir.ui.view">
            <field name="model">mob.extra.image</field>
			<field name="inherit_id" ref="product_multi_image.view_product_image_tree"/>
            <field name="arch" type="xml">
                <field name='variant_id' position='after'>
                    <field name='image_type' widget='many2many'/>
                    <field name='mage_file'/>
                    <field name='mage_product_id'/>
                </field>
			</field>
        </record>
        <record id="view_product_image_form" model="ir.ui.view">
            <field name="model">mob.extra.image</field>
			<field name="inherit_id" ref="product_multi_image.view_product_image_form"/>
            <field name="arch" type="xml">
                <field name='variant_id' position='after'>
                    <field name='image_type' widget='many2many'/>
                    <field name='mage_file'/>
                    <field name='mage_product_id'/>
                </field>
			</field>
        </record>
</data>
</openerp>

